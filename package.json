{
  "name": "storeforge-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:backend": "next dev --turbopack --port 3001",
    "dev:frontend": "next dev --turbopack --port 3000",
    "build": "next build",
    "build:backend": "next build",
    "build:frontend": "next build && next export",
    "start": "next start",
    "start:backend": "next start --port 3001",
    "start:frontend": "next start --port 3000",
    "export": "next export",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "echo 'Running health checks...' && npm run test:health",
    "test:health": "node scripts/test-health.js",
    "test:apis": "node scripts/test-apis.js",
    "test-aliexpress": "node scripts/test-aliexpress-cli.js",
    "test-scrapingbee": "node scripts/test-scrapingbee.js",
    "setup": "node scripts/setup.js",
    "setup-apify": "echo 'Please follow the setup guide in apify-setup-guide.md'",
    "setup-scrapingbee": "echo 'Please follow the setup guide in scrapingbee-setup-guide.md'",
    "deploy": "npm run deploy:check && npm run deploy:backend && npm run deploy:frontend",
    "deploy:check": "node scripts/deploy-check.js",
    "deploy:backend": "echo 'Deploying to Render...' && node scripts/deploy-backend.js",
    "deploy:frontend": "echo 'Deploying to Netlify...' && node scripts/deploy-frontend.js",
    "env:check": "node scripts/check-env.js",
    "env:setup": "node scripts/setup-env.js",
    "db:migrate": "node scripts/db-migrate.js",
    "db:seed": "node scripts/db-seed.js",
    "logs:backend": "node scripts/logs-backend.js",
    "logs:frontend": "node scripts/logs-frontend.js",
    "status": "node scripts/status.js",
    "clean": "rm -rf .next out node_modules/.cache",
    "reset": "npm run clean && npm install"
  },
  "dependencies": {
    "@auth/supabase-adapter": "^1.10.0",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.53.0",
    "apify-client": "^2.15.0",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.6.1",
    "lucide-react": "^0.539.0",
    "next": "15.4.5",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "replicate": "^1.0.1",
    "scrapingbee": "^1.7.5",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
